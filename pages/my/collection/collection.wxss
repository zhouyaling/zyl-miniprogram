page{
  background-color: #F9F8FE;
}

.question-list{
  margin:0;
}

.question-item{
  color: #666;
font-size: 30rpx;
padding:30rpx 20rpx 30rpx 30rpx;
border-bottom:  1px solid #f6f6f6;
background: #fff;
display: flex;
margin-top: 6rpx;
}

.question-item view:nth-of-type(2){
  overflow: hidden;
  text-overflow: ellipsis;
  display: inline-block;
  white-space: nowrap;
}

.item-number{
  display: inline-block;
font-size: 32rpx;
margin-right: 20rpx;
}